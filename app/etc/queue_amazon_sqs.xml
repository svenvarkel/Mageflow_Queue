<?xml version="1.0"?>
<config>
    <global>
        <queue>
            <backend>amazonsqs</backend>
            <amazonsqs>
                <connection>
                    <key>{{AWS SQS KEY}}</key>
                    <secret>{{AWS SQS SECRET}}</secret>
                    <region>{{AWS SQS REGION}}</region>
                </connection>
            </amazonsqs>
        </queue>
    </global>
</config>
